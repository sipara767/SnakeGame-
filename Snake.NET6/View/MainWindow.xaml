<Window x:Class="Snake.NET6.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:localVM="clr-namespace:Snake.NET6.ViewModel"
        xmlns:local="clr-namespace:Snake.NET6.View"
        
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <localVM:MainViewModel/>
    </Window.DataContext>
    <Grid x:Name="Map" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Rectangle  HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top" Width="70" Fill="Black" Height="72" Margin="424,167,0,0"/>
        <Button Command="{Binding MoveRight}" Margin="326,294,326,76"/>

    </Grid>
</Window>
